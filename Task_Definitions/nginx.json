{
    "containerDefinitions": [
      {
        "name": "nginx",
        "image": "nginx:latest",
        "memory": "256",
        "cpu": "256",
        "essential": true,
        "portMappings": [
          {
            "containerPort": 80,
            "protocol": "tcp"
          }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "awslogs-ecs",
                "awslogs-region": "ap-southeast-2",
                "awslogs-stream-prefix": "nginx"
            }
        }
      }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "placementConstraints": [],
    "family": "nginx"
  }